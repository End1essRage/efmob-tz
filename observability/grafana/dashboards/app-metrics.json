{
	"uid": "efmob-app-metrics",
	"title": "EFMOB â€“ App Metrics",
	"timezone": "browser",
	"schemaVersion": 38,
	"version": 1,
	"refresh": "10s",
	"tags": [
		"efmob",
		"metrics"
	],
	"templating": {
		"list": []
	},
	"panels": [
		{
			"id": 1,
			"type": "timeseries",
			"title": "RPS by Method 5min",
			"datasource": "Prometheus",
			"targets": [
				{
					"expr": "sum by (method) (rate(efmob_http_requests_total[5m]))",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 0,
				"w": 12,
				"h": 8
			}
		},
		{
			"id": 2,
			"type": "timeseries",
			"title": "HTTP 5xx Error Rate",
			"datasource": "Prometheus",
			"targets": [
				{
					"expr": "sum(rate(efmob_http_requests_total{status=~\"5..\"}[1m]))",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 12,
				"y": 0,
				"w": 12,
				"h": 8
			}
		},
		{
			"id": 3,
			"type": "timeseries",
			"title": "Latency p95 by Path",
			"datasource": "Prometheus",
			"targets": [
				{
					"expr": "histogram_quantile(0.95, sum by (le, path) (rate(efmob_http_request_duration_seconds_bucket[5m])))",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 8,
				"w": 24,
				"h": 8
			}
		},
		{
			"id": 4,
			"type": "table",
			"title": "Top 5 Slow Endpoints (p95)",
			"datasource": "Prometheus",
			"targets": [
				{
					"expr": "topk(5, histogram_quantile(0.95, sum by (le, path) (rate(efmob_http_request_duration_seconds_bucket[5m]))))",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 16,
				"w": 24,
				"h": 8
			}
		},
		{
			"id": 5,
			"type": "timeseries",
			"title": "Subscriptions Created (rate)",
			"datasource": "Prometheus",
			"targets": [
				{
					"expr": "rate(efmob_subs_created_total[5m])",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 24,
				"w": 12,
				"h": 8
			}
		}
	]
}