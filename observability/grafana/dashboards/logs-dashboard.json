{
	"uid": "efmob-logs",
	"title": "EFMOB â€“ Logs",
	"timezone": "browser",
	"schemaVersion": 38,
	"version": 1,
	"refresh": "10s",
	"tags": [
		"efmob",
		"logs"
	],
	"templating": {
		"list": [
			{
				"name": "request_id",
				"type": "textbox",
				"label": "Request ID",
				"hide": 0
			}
		]
	},
	"panels": [
		{
			"id": 1,
			"type": "logs",
			"title": "All Logs",
			"datasource": "Loki",
			"targets": [
				{
					"expr": "{service_name=\"unknown_service\"}",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 0,
				"w": 24,
				"h": 8
			}
		},
		{
			"id": 2,
			"type": "logs",
			"title": "Error Logs",
			"datasource": "Loki",
			"targets": [
				{
					"expr": "{service_name=\"unknown_service\"} | level = \"error\"",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 8,
				"w": 24,
				"h": 8
			}
		},
		{
			"id": 3,
			"type": "logs",
			"title": "Logs by Request ID",
			"datasource": "Loki",
			"targets": [
				{
					"expr": "{service_name=\"unknown_service\"} | json | request_id=\"{{request_id}}\"",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 16,
				"w": 24,
				"h": 8
			}
		},
		{
			"id": 4,
			"type": "logs",
			"title": "HTTP 5xx Requests",
			"datasource": "Loki",
			"targets": [
				{
					"expr": "{service_name=\"unknown_service\"} | json | status >= 500",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 24,
				"w": 24,
				"h": 8
			}
		},
		{
			"id": 5,
			"type": "logs",
			"title": "Slow Requests (>1s)",
			"datasource": "Loki",
			"targets": [
				{
					"expr": "{service_name=\"unknown_service\"} | json | duration_ms > 1000",
					"refId": "A"
				}
			],
			"gridPos": {
				"x": 0,
				"y": 32,
				"w": 24,
				"h": 8
			}
		}
	]
}